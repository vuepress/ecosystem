{
  "name": "@vuepress/plugin-medium-zoom",
  "version": "2.0.0-rc.123",
  "description": "VuePress plugin - medium-zoom",
  "keywords": [
    "image",
    "medium",
    "plugin",
    "vuepress",
    "vuepress-plugin",
    "zoom"
  ],
  "homepage": "https://ecosystem.vuejs.press/plugins/features/medium-zoom.html",
  "bugs": {
    "url": "https://github.com/vuepress/ecosystem/issues"
  },
  "license": "MIT",
  "author": "meteorlxy",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuepress/ecosystem.git",
    "directory": "plugins/features/plugin-medium-zoom"
  },
  "files": [
    "lib"
  ],
  "type": "module",
  "sideEffects": [
    "./lib/client/styles/**/*.css"
  ],
  "exports": {
    ".": {
      "types": "./lib/node/index.d.ts",
      "default": "./lib/node/index.js"
    },
    "./client": {
      "types": "./lib/client/index.d.ts",
      "default": "./lib/client/index.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown --config-loader unrun",
    "clean": "rimraf --glob ./lib ./*.tsbuildinfo",
    "style": "sass src:lib --embed-sources --style=compressed --pkg-importer=node"
  },
  "dependencies": {
    "@vuepress/helper": "workspace:*",
    "medium-zoom": "^1.1.0",
    "vue": "catalog:"
  },
  "peerDependencies": {
    "vuepress": "catalog:"
  }
}
