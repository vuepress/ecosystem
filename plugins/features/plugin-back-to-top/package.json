{
  "name": "@vuepress/plugin-back-to-top",
  "version": "2.0.0-rc.123",
  "description": "VuePress plugin - back to top",
  "keywords": [
    "plugin",
    "top",
    "vuepress",
    "vuepress-plugin"
  ],
  "homepage": "https://ecosystem.vuejs.press/plugins/features/back-to-top.html",
  "bugs": {
    "url": "https://github.com/vuepress/ecosystem/issues"
  },
  "license": "MIT",
  "author": "meteorlxy",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuepress/ecosystem.git",
    "directory": "plugins/features/plugin-back-to-top"
  },
  "files": [
    "lib"
  ],
  "type": "module",
  "sideEffects": [
    "./lib/styles/*.css"
  ],
  "exports": {
    ".": {
      "types": "./lib/node/index.d.ts",
      "default": "./lib/node/index.js"
    },
    "./client": {
      "types": "./lib/client/index.d.ts",
      "default": "./lib/client/index.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown --config-loader unrun",
    "clean": "rimraf --glob ./lib ./*.tsbuildinfo",
    "copy": "cpx \"src/**/*.svg\" lib",
    "style": "sass src:lib --embed-sources --style=compressed --pkg-importer=node"
  },
  "dependencies": {
    "@vuepress/helper": "workspace:*",
    "@vueuse/core": "catalog:",
    "vue": "catalog:"
  },
  "peerDependencies": {
    "vuepress": "catalog:"
  }
}
