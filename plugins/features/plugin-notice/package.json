    "copy": "cpx \"src/**/*.svg\" lib",
    "style": "sass src:lib --embed-sources --style=compressed"
  },
  "dependencies": {
    "@vuepress/helper": "workspace:*",
    "@vueuse/core": "^11.2.0",
    "vue": "^3.5.13"
