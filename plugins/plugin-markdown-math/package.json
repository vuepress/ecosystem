{
  "name": "@vuepress/plugin-markdown-math",
  "version": "2.0.0-rc.30",
  "description": "VuePress plugin - markdown math",
  "keywords": [
    "vuepress-plugin",
    "vuepress",
    "plugin",
    "markdown",
    "math"
  ],
  "homepage": "https://ecosystem.vuejs.press/plugins/markdown-math.html",
  "bugs": {
