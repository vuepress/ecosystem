    "vuepress-next"
  ],
  "license": "MIT",
  "dependencies": {
    "@vuepress/utils": "2.0.0-rc.20",
    "@hyrious/esbuild-plugin-commonjs": "0.2.5",
    "meilisearch-docsearch": "^0.7.1",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "cpx2": "8.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-vuepress": "5.2.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3",
    "vuepress": "2.0.0-rc.20"
