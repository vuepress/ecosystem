{
  "name": "vuepress-plugin-meilisearch2",
  "description": "Added the meilisearch search plugin to Vuepress2.x",
  "version": "0.5.1",
  "module": "./lib/index.js",
  "exports": {
    ".": {
      "import": "./lib/index.js"
    }
  },
  "files": [
    "lib"
  ],
