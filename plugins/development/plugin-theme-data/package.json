{
  "name": "@vuepress/plugin-theme-data",
  "version": "2.0.0-rc.123",
  "description": "VuePress plugin - theme data",
  "keywords": [
    "plugin",
    "theme",
    "vuepress",
    "vuepress-plugin"
  ],
  "homepage": "https://ecosystem.vuejs.press/plugins/development/theme-data.html",
  "bugs": {
    "url": "https://github.com/vuepress/ecosystem/issues"
  },
  "license": "MIT",
  "author": "meteorlxy",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuepress/ecosystem.git",
    "directory": "plugins/development/plugin-theme-data"
  },
  "files": [
    "lib"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "./lib/node/index.js",
  "types": "./lib/node/index.d.ts",
  "exports": {
    ".": "./lib/node/index.js",
    "./client": "./lib/client/index.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "bundle": "rollup -c rollup.config.ts --configPlugin esbuild",
    "clean": "rimraf --glob ./lib ./*.tsbuildinfo",
    "copy": "cpx \"src/**/*.d.ts\" lib"
  },
  "dependencies": {
    "@vue/devtools-api": "^8.0.6",
    "vue": "catalog:"
  },
  "peerDependencies": {
    "vuepress": "catalog:"
  }
}
