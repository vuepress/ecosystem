{
  "name": "@vuepress/plugin-markdown-image",
  "version": "2.0.0-rc.78",
  "description": "VuePress plugin - markdown image",
  "keywords": [
    "vuepress-plugin",
    "vuepress",
    "plugin",
    "markdown",
    "image",
    "figure",
