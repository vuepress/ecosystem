  },
  "type": "module",
  "exports": {
    ".": "./lib/node/index.js",
    "./figure.css": "./lib/client/styles/figure.css",
    "./mark.css": "./lib/client/styles/mark.css",
    "./package.json": "./package.json"
  },
  "main": "./lib/node/index.js",
  "types": "./lib/node/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
