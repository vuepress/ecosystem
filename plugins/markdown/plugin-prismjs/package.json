{
  "name": "@vuepress/plugin-prismjs",
  "version": "2.0.0-rc.123",
  "description": "VuePress plugin - prismjs",
  "keywords": [
    "highlight",
    "plugin",
    "prismjs",
    "vuepress",
    "vuepress-plugin"
  ],
  "homepage": "https://ecosystem.vuejs.press/plugins/markdown/prismjs.html",
  "bugs": {
    "url": "https://github.com/vuepress/ecosystem/issues"
  },
  "license": "MIT",
  "author": "meteorlxy",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuepress/ecosystem.git",
    "directory": "plugins/markdown/plugin-prismjs"
  },
  "files": [
    "lib"
  ],
  "type": "module",
  "sideEffects": [
    "./lib/client/styles/**/*.css"
  ],
  "main": "./lib/node/index.js",
  "types": "./lib/node/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/node/index.d.ts",
      "default": "./lib/node/index.js"
    },
    "./styles/*.css": "./lib/client/styles/*.css",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown --config-loader unrun",
    "clean": "rimraf --glob ./lib ./*.tsbuildinfo",
    "style": "unrun scripts/generateThemeFiles.ts"
  },
  "dependencies": {
    "@vuepress/helper": "workspace:*",
    "@vuepress/highlighter-helper": "workspace:*",
    "prismjs": "^1.30.0"
  },
  "devDependencies": {
    "@types/markdown-it": "^14.1.2",
    "@types/prismjs": "^1.26.6",
    "markdown-it": "^14.1.1"
  },
  "peerDependencies": {
    "vuepress": "catalog:"
  }
}
