{
  "name": "@vuepress/plugin-shiki",
  "version": "2.0.0-rc.123",
  "description": "VuePress plugin - shiki",
  "keywords": [
    "highlight",
    "plugin",
    "shiki",
    "vuepress",
    "vuepress-plugin"
  ],
  "homepage": "https://ecosystem.vuejs.press/plugins/markdown/shiki.html",
  "bugs": {
    "url": "https://github.com/vuepress/ecosystem/issues"
  },
  "license": "MIT",
  "author": "meteorlxy",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuepress/ecosystem.git",
    "directory": "plugins/markdown/plugin-shiki"
  },
  "files": [
    "lib"
  ],
  "type": "module",
  "sideEffects": [
    "./lib/client/styles/**/*.css"
  ],
  "main": "./lib/node/index.js",
  "types": "./lib/node/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/node/index.d.ts",
      "default": "./lib/node/index.js"
    },
    "./resolveLang": "./lib/node/resolveLang.js",
    "./shiki.css": "./lib/client/styles/shiki.css",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown --config-loader unrun",
    "clean": "rimraf --glob ./lib ./*.tsbuildinfo",
    "style": "sass src:lib --embed-sources --style=compressed --pkg-importer=node"
  },
  "dependencies": {
    "@shikijs/transformers": "^3.23.0",
    "@vuepress/helper": "workspace:*",
    "@vuepress/highlighter-helper": "workspace:*",
    "nanoid": "^5.1.6",
    "shiki": "^3.23.0",
    "synckit": "^0.11.12"
  },
  "devDependencies": {
    "@types/markdown-it": "^14.1.2",
    "@vuepress/shiki-twoslash": "workspace:*",
    "markdown-it": "^14.1.1"
  },
  "peerDependencies": {
    "@vuepress/shiki-twoslash": "workspace:*",
    "vuepress": "catalog:"
  },
  "peerDependenciesMeta": {
    "@vuepress/shiki-twoslash": {
      "optional": true
    }
  }
}
